hone.length > 0 && cleanPhone.length <= 11) {\r\n            \/\/ Adiciona o c\u00f3digo do pa\u00eds +55 para formato E.164\r\n            return `+55${cleanPhone}`;\r\n        }\r\n\r\n        \/\/ Se o n\u00famero tiver mais de 11 d\u00edgitos, retorna vazio (ou pode ajustar para outro comportamento)\r\n        return '';\r\n    }\r\n\r\n    \/\/ Quando o formul\u00e1rio for submetido, formata os n\u00fameros no formato E.164\r\n    $(\"form\").submit(function() {\r\n        phoneInputField.forEach((input) => {\r\n            \/\/ Pega o valor atual do campo de telefone\r\n            const phoneValue = input.value.trim();\r\n            \r\n            \/\/ Formata o n\u00famero para E.164\r\n            const formattedNumber = formatPhoneNumber(phoneValue);\r\n            \r\n            \/\/ Substitui o valor do campo com o n\u00famero formatado\r\n            $(input).val(formattedNumber);\r\n        });\r\n    });\r\n<\/script>\r\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<div class=\"elementor-element elementor-element-0a4b550 elementor-widget elementor-widget-text-editor\" data-id=\"0a4b550\" data-element_type=\"widget\" data-widget_type=\"text-editor.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t\t\t\t\t<p>OBS: N\u00c3O CONFIE EM ABORDAGENS FEITAS POR N\u00daMEROS N\u00c3O OFICIAIS<\/p>\t\t\t\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"elementor-element elementor-element-e71f2f7 e-flex e-con-boxed e-con e-parent\" data-id=\"e71f2f7\" data-element_type=\"container\" data-settings=\"{&quot;background_background&quot;:&quot;classic&quot;,&quot;_ha_eqh_enable&quot;:false}\">\n\t\t\t\t\t<div class=\"e-con-inner\">\n\t\t\t\t<div class=\"elementor-element elementor-element-65209d0 elementor-widget__width-inherit elementor-widget elementor-widget-heading\" data-id=\"65209d0\" data-element_type=\"widget\" data-widget_type=\"heading.default\">\n\t\t\t\t<div cla